<h1>Se connecter</h1>

<%= form_tag sessions_path, method: "post" do %>

<%= label_tag 'Adresse email' %>
<%= text_field_tag 'email', @user.email %>
<br/>
<br/>

<%= label_tag 'Mot de passe' %>
<%= text_field_tag 'password', @user.password %>
<br/>
<br/>

<%= submit_tag "Se connecter" %>

<% end %>
